{
  "name": "@sargunv/prettier-config",
  "version": "0.2.2",
  "homepage": "https://github.com/sargunv/prettier-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/sargunv/prettier-config.git"
  },
  "license": "Apache-2.0",
  "type": "commonjs",
  "main": "index.js",
  "files": [
    "./index.js"
  ],
  "scripts": {
    "build": "true",
    "configure": "yarn dlx @yarnpkg/sdks vscode && ignore-sync .",
    "fix": "eslint . --fix && prettier --write .",
    "lint": "tsc --noEmit && eslint . && prettier --check . --loglevel warn",
    "prepack": "run build",
    "prepublish": "run lint && run test",
    "test": "true"
  },
  "prettier": "@sargunv/prettier-config",
  "eslintConfig": {
    "extends": [
      "@sargunv/eslint-config",
      "@sargunv/eslint-config/mixins/typescript"
    ],
    "root": true
  },
  "dependencies": {
    "prettier-plugin-packagejson": "^2.4.2"
  },
  "devDependencies": {
    "@sargunv/eslint-config": "^0.2.0",
    "@sargunv/typescript-config": "^0.1.0",
    "@types/node": "^18.11.19",
    "@types/prettier": "^2.7.2",
    "eslint": "^8.33.0",
    "ignore-sync": "^6.0.2",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "prettier": "*"
  },
  "packageManager": "yarn@4.0.0-rc.38",
  "languageName": "node",
  "publishConfig": {
    "access": "public"
  }
}
